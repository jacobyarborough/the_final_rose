<h1>Contestants' Info</h1>
<div>
<% @contestants.each do |contestant| %>
  Name: <%= link_to(contestant.name, bachelorette_contestant_path(contestant.bachelorette_id,contestant.id)) %><br>
  Age: <%= contestant.age %><br>
  Hometown: <%= contestant.hometown %><br>
<% end %>
</div>
<div id="home-town">
<% @contestants.hometowns(@contestants.first.bachelorette_id).each do |place| %>
  <%= place %>
<% end %>
</div>